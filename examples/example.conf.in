[General]
	daemon = false
	worker_threads = 4
	hostname = localhost:8080
	uid = 99
	gid = 99

[Http]
	port = 8080
	use_tls = true
	tls_cert_file = @CMAKE_CURRENT_SOURCE_DIR@/tlscert.crt
	tls_key_file = @CMAKE_CURRENT_SOURCE_DIR@/tlscert.key
	session_lifetime = 3600
	session_max_lifetime = -1
	timeout = 15
	request_max_size = 1048576

[Websocket]
	message_max_size = 1048576
